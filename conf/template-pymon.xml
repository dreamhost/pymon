<config>
    <enabled>
        <groups_or_services>services</groups_or_services>
        <services>
            <service_type>ping</service_type>
        </services>
        <groups>
        </groups>
    </enabled>
    <disabled>
        <groups_or_services>services</groups_or_services>
        <services>
            <service_type>http status</service_type>
            <service_type>http text</service_type>
            <service_type>local process</service_type>
            <service_type>dns dig</service_type>
        </services>
    </disabled>
    <services>
        <service type="ping">
            <defaults>
                <count>4</count>
                <interval>5</interval>
                <service_name>connectivity</service_name>
                <message_template>There was a %s%% ping return from host %s</message_template>
                <binary>/sbin/ping</binary>
                <sent_message>Sent ping notice email message to %s</sent_message>
                <threshold_type>ranged</threshold_type>
                <ok>
                    <threshold>96,100</threshold>
                    <message>pymon OK: %s : %s%% loss</message>
                </ok>
                <recovering>
                    <threshold>NA</threshold>
                    <message>pymon RECOVERING: %s : %s%% loss</message>
                </recovering>
                <warn>
                    <threshold>26,95</threshold>
                    <message>pymon WARNING: %s : %s%% loss</message>
                </warn>
                <error>
                    <threshold>0,25</threshold>
                    <message>pymon ERROR: %s : %s%% loss</message>
                </error>
            </defaults>
            <entries>
                <entry uri="shell1.adytum.us" enabled="true">
                    <escalation enabled="false">
                        <group level="0">
                            <maillist>
                                <email>oubiwann@myrealbox.com</email>
                                <email>oubiwann@yahoo.com</email>
                            </maillist>
                        </group>
                        <group level="1">
                            <maillist></maillist>
                        </group>
                    </escalation>
                    <scheduled_downtime></scheduled_downtime>
                </entry>
                <entry uri="shell2.adytum.us" enabled="false">
                    <escalation enabled="false">
                    </escalation>
                </entry>
                <entry uri="svn.adytum.us" enabled="true">
                    <escalation enabled="false">
                        <group level="0">
                            <maillist>
                                <email>oubiwann@myrealbox.com</email>
                                <email>oubiwann@yahoo.com</email>
                            </maillist>
                        </group>
                        <group level="1">
                            <maillist></maillist>
                        </group>
                    </escalation>
                </entry>
                <entry uri="divorce-md.com" enabled="false">
                    <escalation enabled="false">
                    </escalation>
                </entry>
            </entries>
        </service>
        <service type="dns dig">
            <defaults>
                <binary>/usr/bin/dig</binary>
                <interval>21600</interval>
                <interval>20</interval>
                <timeout>10</timeout>
                <message_template>Line %s of %s was %s as expected: name server %s returned %s for host %s</message_template>
                <sent_message>Sent dns dig notice email message to %s</sent_message>
            </defaults>
            <entries>
                <entry uri="mx@a.ns.adytumsolutions.com/adytumsolutions.com" enabled="True">
                    <expected>
                        <line>0 a.mx.adytumsolutions.com.</line>
                        <line>50 mdpawl01.adytumsolutions.com.</line>
                    </expected>
                    <escalation enabled="false">
                    </escalation>
                </entry>
                <entry uri="mx@b.ns.adytumsolutions.com/adytumsolutions.com" enabled="True">
                    <expected>
                        <line>0 a.mx.adytumsolutions.com.</line>
                        <line>50 mdpawl01.adytumsolutions.com.</line>
                    </expected>
                    <escalation enabled="false">
                    </escalation>
                </entry>
                <entry uri="a@a.ns.adytumsolutions.com/adytumsolutions.com" enabled="True">
                    <expected>
                        <line>204.91.156.55</line>
                    </expected>
                    <escalation enabled="false">
                    </escalation>
                </entry>
                <entry uri="a@b.ns.adytumsolutions.com/adytumsolutions.com" enabled="True">
                    <expected>
                        <line>204.91.156.55</line>
                    </expected>
                    <escalation enabled="false">
                    </escalation>
                </entry>
                <entry uri="a@a.ns.adytumsolutions.com/shell2.adytumsolutions.com" enabled="True">
                    <expected>
                        <line>63.89.76.163</line>
                    </expected>
                    <escalation enabled="false">
                    </escalation>
                </entry>
                <entry uri="reverse@a.ns.adytumsolutions.com/63.89.76.163" enabled="True">
                    <expected>
                        <line>mdpawl04.adytumsolutions.com.</line>
                    </expected>
                    <escalation enabled="false">
                    </escalation>
                </entry>
            </entries>
        </service>
        <service type="http status">
            <defaults>
                <service_name>HTTP Status</service_name>
                <interval>5</interval>
                <http_port>80</http_port>
                <message_template>The web server resource '%s' returned HTTP status %s</message_template>
                <sent_message>Sent http status notice email message to %s</sent_message>
                <threshold_type>listed</threshold_type>
                <ok>
                    <threshold>100, 101, 200, 201, 202, 203, 300, 302, 303, 304, 305</threshold>
                    <message></message>
                </ok>
                <recover>
                    <threshold>NA</threshold>
                    <message></message>
                </recover>
                <warn>
                    <threshold>204, 205, 206, 301, 401, 402, 403, 405, 406, 407, 411, 412, 413, 414, 415, 416</threshold>
                    <message></message>
                </warn>
                <error>
                    <threshold>400, 404, 408, 409, 410, 417, 500, 501, 502, 503, 504, 505</threshold>
                    <message></message>
                </error>
            </defaults>
            <entries>
                <entry uri="localhost" enabled="True">
                    <client>None</client>
                </entry>
                <entry uri="adytum.us" enabled="True">
                    <client>None</client>
                </entry>
                <entry uri="www.divorce-md.com" enabled="True">
                    <client>None</client>
                </entry>
                <entry uri="accuratemachinery.com" enabled="True">
                    <client>None</client>
                </entry>
            </entries>
        </service>
        <service type="local process">
            <defaults>
            </defaults>
        </service>
    </services>
    <system>
        <uid>pymon</uid>
        <gid>pymon</gid>
        <install_prefix>%%INSTALL_PREFIX%%</install_prefix>
        <config_dir>%%CONFIG_DIR%%</config_dir>
        <config_file>%%CONFIG_FILE%%</config_file>
        <pymon_app>%%PYMON_APP%%</pymon_app>
        <twistd>%%TWISTED_DAEMON%%</twistd>
        <host_name>codawx01</host_name>
        <instance_name>pymon test</instance_name>
        <web>
            <port>8080</port>
            <agent_string>pymon Enterprise Monitoring (http://pymon.sf.net)</agent_string>
        </web>
        <database>
            <dbtype>mysql</dbtype>
            <directory>data/zodb</directory>
            <host>localhost</host>
            <port></port>
            <user></user>
            <password></password>
            <classification>sql</classification>
            <dbname>pymon_testdb</dbname>
        </database>
        <backups>
            <state_data>
                <interval>300</interval>
                <directory>data</directory>
                <filename>backup.pkl</filename>
            </state_data>
        </backups>
        <mail>
            <from_address>pymon@adytum.us</from_address>
            <sendmail>/usr/sbin/sendmail</sendmail>
        </mail>
        <agents>
            <port>10999</port>
        </agents>
    </system>
    <constants>
        <states>
            <unknown>-1</unknown>
            <ok>1</ok>
            <recovering>2</recovering>
            <warn>3</warn>
            <error>4</error>
        </states>
    </constants>
    <global_names>
        <state>state</state>
        <history>history</history>
        <factories>factories</factories>
    </global_names>
</config>
